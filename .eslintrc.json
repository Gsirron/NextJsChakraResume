{
  "extends": [
    "sznm/react",
    "plugin:react/jsx-runtime",
    "plugin:@next/next/recommended",
    "plugin:import/typescript"
    // "airbnb",
    // "airbnb-typescript"
  ],
  "rules": {
    "react/jsx-curly-brace-presence": [1, { "props": "always" }],

    "import/extensions": [
      "error",
      "never",
      {
        "js": "never",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
    ]
  }
}
